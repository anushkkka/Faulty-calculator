n1=int(input("enter first number"))
n2=int(input("enter second number"))
op=input()
if(op=="+"):
    print(n1+n2)
    if(n1==56 and n2==9):
        print("77")
elif(op=="-"):
    print(n1-n2)
elif(op=="*"):
    print(n1*n2)
    if(n1==56 and n2==9):
        print("555")
elif(op=="/"):
    print(n1/n2)
    if(n1==56 and n2==6):
        print("4")
else:
    print("n1%n2")
